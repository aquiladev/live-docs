[comment]: <> (This is an auto-generated table with CNS smart-contract addresses. See README for more info.)
<table>
    <th>Network</th>
    <th>Contract address</th>
    <% for (const row of rows) { %>
    <tr>
        <td><%= row.network %></td>
        <td><a href="<%= row.blockchainExplorerURL %>/address/<%= row.address %>"><%= row.address %></a></td>
    </tr>
    <% } %>
</table>

<% if (hasLegacyAddresses) { %>
<br>
<table>
    <th>Network</th>
    <th>Legacy addresses</th>
    <% for (const row of rows) {%>
    <% if(row.legacyAddresses.length === 0) { continue; } %>
    <tr>
        <td><%= row.network %></td>
        <% let legacyAddresses = row.legacyAddresses; %>
           <td><% for (const address of legacyAddresses) { %><a
                    href="<%= row.blockchainExplorerURL %>/address/<%= address %>"><%= address %></a>
               <%}%>
            </td>
    </tr>
    <% } %>
</table>
<% } %>
